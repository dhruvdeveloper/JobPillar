    <script>
        $(function () {
            $('#radio-student').click(function () {
                $('#radio-student-input').css("display", "block");
                $('#radio-student-lable').css("display", "block");
            });
            $('#radio-faculty').click(function () {
                $('#radio-student-input').css("display", "none");
                $('#radio-student-lable').css("display", "none");
            });
        });
    </script>

    <script>
        $(function () {
            $('#add-button-id').click(function () {
                const temp = "<tr>" +
                    "<td></td>" +
                    "<td></td>" +
                    "<td></td>" +
                    "<td></td>" +
                    "</tr>";
                $('#tbody-id').append(temp)
            })
        });
    </script>

    <script>
        $(".nav .nav-link").on("click", function () {
            $(".nav").find(".active").removeClass("active");
            $(this).addClass("active");
        });
    </script>
